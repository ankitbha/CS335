MODULE loop;
VAR i, j, k, res : INTEGER;

BEGIN

    res := 0;
    i := 0;
    j := 0;
    k := 0;
    WHILE ( i < 10 ) DO
        WHILE ( j < 10 ) DO
            WHILE ( k < 10 ) DO
                res := res+1;
                k := k+1;
            END;
        j := j+1;
        END;
    i := i+1;
    END;

    WRITE("res=");
    WRITELN(res);
END loop.
